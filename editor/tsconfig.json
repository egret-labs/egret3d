{
    "compilerOptions": {
        "target": "es5",
        "lib": [
            "es5",
            "dom",
            "es2015.promise"
        ],
        "sourceMap": false,
        "declaration": true,
        "stripInternal": true,
        "alwaysStrict": true,
        "skipLibCheck": true,
        "noImplicitAny": false,
        "noImplicitThis": false,
        "noUnusedLocals": false,
        "noUnusedParameters": false,
        "strictNullChecks": false,
        "experimentalDecorators": true,
        "allowSyntheticDefaultImports": false,
        "preserveConstEnums": true,
        "outFile": "bin/editor/editor.js"
    },
    "include": [
        "../core/libs/",
        "../core/bin/",
        "src",
        "libs"
    ]
}